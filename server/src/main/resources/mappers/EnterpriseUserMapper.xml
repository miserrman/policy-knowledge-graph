<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.longyan.policy.mapper.EnterpriseUserMapper">
    <select id="findEnterpriseUserByUserId" resultType="EnterpriseUser" parameterType="Integer">
        select id,user_id,enterprise_id from enterprise_user
        where user_id=#{userId}
    </select>
</mapper>
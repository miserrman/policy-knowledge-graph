<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.longyan.policy.mapper.IndustryMapper">
    <select id="findIndustryById" resultType="Industry" parameterType="Integer">
        select id,name,keywords from industry
        where id=#{id}
    </select>
</mapper>